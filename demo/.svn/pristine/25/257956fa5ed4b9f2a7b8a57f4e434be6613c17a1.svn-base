	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#dddddd',endColorstr='#eeeeee', GradientType=0 );
}
</style>

</head>

<body style="padding: 10px">

	<form name="form" method="post" action="" id="form">
		<div id="errorLabelContainer" class="l-text-invalid"></div>
		<div>
			<input id="stuid" value="${student.id }" type="hidden" />
		</div>
		<table cellpadding="0" cellspacing="0" class="l-table-edit">

			<tr>
				<td align="right" class="l-table-edit-td">英文名:</td>
				<td align="left" class="l-table-edit-td"><input
					name="firstname" type="text" id="firstname" ltype="text"
					value="${student.firstname }"
					validate="{required:true,minlength:1,maxlength:20}" />
				</td>
				<td align="left"></td>
				<td align="right" class="l-table-edit-td">英文姓:</td>
				<td align="left" class="l-table-edit-td"><input name="lastname"
					type="text" id="lastname" ltype="text" value="${student.lastname }"
					validate="{required:true,minlength:1,maxlength:20}" />
				</td>
				<td align="left"></td>
			</tr>
			<tr>
				<td align="right" class="l-table-edit-td">中文名:</td>
				<td align="left" class="l-table-edit-td"><input name="stuname"
					type="text" id="stuname" ltype="text" value="${student.stuname }" />
				</td>
				<td align="left"></td>
				<td align="right" class="l-table-edit-td">国家:</td>
				<td align="left" class="l-table-edit-td"><input name="nation"
					type="text" id="nation" ltype="text" value="${student.nation }"
					validate="{required:true,minlength:1,maxlength:20}" />
				</td>
				<td align="left"></td>
			</tr>
			<tr>
				<td align="right" class="l-table-edit-td">护照号:</td>
				<td align="left" class="l-table-edit-td"><input name="passport"
					type="text" value="${student.passport }" id="passport" ltype="text"
					placeholder="请输入护照号"
					validate="{required:true,minlength:1,maxlength:20}" />
				</td>
				<td align="left"></td>
				<!-- onblur="check()" -->
				<td align="right" class="l-table-edit-td">学号:</td>
				<td align="left" class="l-table-edit-td"><input name="stunum"
					type="text" id="stunum" value="${student.stuid }"
					disabled="disabled" ltype="text" />
				</td>
				<td align="left"></td>
			</tr>
			<tr>
				<td align="right" class="l-table-edit-td">联系方式:</td>
				<td align="left" class="l-table-edit-td"><input
					name="telephone" type="text" id="telephone"
					value="${student.telephone }" ltype="text"
					validate="{required:true,minlength:1,maxlength:20}" />
				</td>
				<td align="left"></td>
				<td align="right" class="l-table-edit-td">班级:</td>
				<td align="left" class="l-table-edit-td"><input id="classid"
					value="${student.classes.id }" type="hidden" /> <input
					value="${student.classes.classname }" disabled="disabled" /></td>
			</tr>
			<tr>
				<td align="right" class="l-table-edit-td">学生类别:</td>
				<td align="left" class="l-table-edit-td"><input
					name="studenttype" type="text" id="studenttype"
					value="${student.studentType }" disabled="disabled" ltype="text"
					validate="{required:true,minlength:1,maxlength:20}" />
				</td>
				<td align="left"></td>
				<td align="right" class="l-table-edit-td">专业:</td>
				<td align="left" class="l-table-edit-td"><input name="major"
					type="text" id="major" ltype="text"
					value="${student.classes.major }" disabled="disabled"
					validate="{required:true,minlength:1,maxlength:20}" />
				</td>
				<td align="left"></td>
			</tr>
			<tr>
				<td align="right" class="l-table-edit-td">住宿地点:</td>
				<td align="left" class="l-table-edit-td"><input
					name="roomplace" type="text" value="${student.roomplace }"
					id="roomplace" ltype="text"
					validate="{required:true,minlength:1,maxlength:20}" />
				</td>
				<td align="left"></td>
				<td align="right" class="l-table-edit-td">学生来源:</td>
				<td align="left" class="l-table-edit-td"><input name="source"
					type="text" id="source" value="${student.source }"
					disabled="disabled" ltype="text"
					validate="{required:true,minlength:1,maxlength:20}" />
				</td>
				<td align="left"></td>
			</tr>
			<!-- <tr>
				<td align="right" class="l-table-edit-td">照片:</td>
				<th align="left" class="l-table-edit-td" colspan="3">
					<div class="uploader white">						
							<input type="text" class="filename" readonly />							
							<input type="file" name="image" size="30" />						
					</div></th>
				<td align="left"><input type="button"  onclick="changeimg()" class="button" value="上传..."/></td>
				
			</tr> -->
			<tr>
				<td align="right" class="l-table-edit-td"></td>
				<td align="left"><div id="result2" style="color: red;"></div>
				</td>
				<td align="left" class="l-table-edit-td"></td>

			</tr>
			<tr>
				<th align="center" class="l-table-edit-td" colspan="6"><div
						id="result" style="color: red;"></div>
				</th>
			</tr>
			<tr>
				<td align="right" class="l-table-edit-td"></td>
				<td align="left" class="l-table-edit-td"></td>
				<th align="left" colspan="2">
				<input class="l-button l-button-submit" type="submit" value="更&nbsp;&nbsp;新" />
				</th>
				<td align="left" class="l-table-edit-td"></td>
				<td align="left"></td>
			</tr>
		</table>
		<br />
	</form>
</body>
</html>