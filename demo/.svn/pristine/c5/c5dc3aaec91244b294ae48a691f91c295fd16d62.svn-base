	@Override
	public void roomaddbytxt(String path) {

		try {
			List<String> linelist = readTxtFile(path);
			for (String line : linelist) {
				StringTokenizer tokenizer = new StringTokenizer(line);
				if (tokenizer.hasMoreTokens()) {
					// String s = tokenizer.nextToken();
					Classroom classroom = new Classroom();
					String num = tokenizer.nextToken();
					List<Classroom> clsroom = studentDao.loadclassroombynum(num);
					if (clsroom.size()==0) {
						classroom.setNum(num);
						classroom.setRoomname(tokenizer.nextToken());
						classroom.setRoomplace(tokenizer.nextToken());
						classroom.setRoomtype(tokenizer.nextToken());
						classroom.setRoomcapacity(Integer.valueOf(tokenizer.nextToken()));
						classroom.setRealcapacity(Integer.valueOf(tokenizer.nextToken()));
						classroom.setIsavaible(0);
						studentDao.roomadd(classroom);
					}else {
						continue;
					}
					
				}
			}
		} catch (Exception e) {
			e.printStackTrace();
		}
		File file = new File(path);
		if (file.exists()) {
			file.delete();
		}
	}
